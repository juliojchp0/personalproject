    <servlet>
        <servlet-name>ControladorTodosModificar</servlet-name>
        <servlet-class>paquete.ControladorTodosModificar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorBorrarUsuario</servlet-name>
        <servlet-class>paquete.ControladorBorrarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorHorarios</servlet-name>
        <servlet-class>paquete.ControladorHorarios</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControladorPrincipal</servlet-name>
        <url-pattern>/ControladorPrincipal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorRegistro2</servlet-name>
        <url-pattern>/ControladorRegistro2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorReserva</servlet-name>
        <url-pattern>/ControladorReserva</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorDesReserva</servlet-name>
