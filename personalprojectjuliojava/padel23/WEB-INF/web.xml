<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ControladorPrincipal</servlet-name>
        <servlet-class>paquete.ControladorPrincipal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorRegistro2</servlet-name>
        <servlet-class>paquete.ControladorRegistro2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorReserva</servlet-name>
        <servlet-class>paquete.ControladorReserva</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorDesReserva</servlet-name>
